{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API Desenvolvida utilizando TS, NODE, ES6, Postgresql e Sequelize",
  "main": "server.ts",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "NODE_ENV=development node ./build/server/server.js",
    "test-db": "NODE_ENV=test node ./build/server/server.js",
    "unit-test": "NODE_ENV=test mocha --opts ./build/tests/unit/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts ./build/tests/integration/mocha.opts ./build/tests/integration/integration.test.js"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "ES6"
  ],
  "author": "Diego Moraes",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "http": "0.0.0",
    "http-status": "^1.3.1",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.0",
    "@types/http-status": "^0.2.30",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/sequelize": "^4.27.33",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "sequelize": "^4.42.0",
    "supertest": "^3.3.0",
    "testdouble": "^3.9.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  }
}
